    AssertEqual
    \ [0, [[9, 1000001, 'ALEWarningSign']]],
    \ ale#sign#ParseSigns([
    \   'Signs for app.js:',
    \   '    line=9  id=1000001  group=ale  name=ALEWarningSign',
    \ ])
  else
    AssertEqual
    \ [0, [[9, 1000001, 'ALEWarningSign']]],
    \ ale#sign#ParseSigns([
    \   'Signs for app.js:',
    \   '    line=9  id=1000001  name=ALEWarningSign',
    \ ])
  endif

