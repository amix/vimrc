snippet +
	(+	${1}
		${0})

snippet -
	(-	${1}
		${0})

snippet /
	(/	${1}
		${0})

snippet *
	(*	${1}
		${0})

# Definition
snippet def
	(define (${1:name})
			(${0:definition}))

# Definition with lambda
snippet defl
	(define ${1:name}
			(lambda (x)(${0:definition})))

# Condition
snippet cond
	(cond	((${1:predicate}) (${2:action}))
			((${3:predicate}) (${0:action})))

# If statement
snippet if
	(if (${1:predicate})
		(${2:true-action})
		(${0:false-action}))
